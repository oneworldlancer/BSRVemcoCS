                if (code == "delete") {


                    $("#toast_Message").removeClass('bg-success');
                    $("#toast_Message").removeClass('bg-warning');
                    $("#toast_Message").removeClass('bg-info');
                    $("#toast_Message").removeClass('bg-danger');

                    $("#toast_Message").addClass('bg-danger');

                    $("#toast_Message").html("Item deleted successfully.");

                }

                else if (code == "new") {


                    $("#toast_Message").removeClass('bg-success');
                    $("#toast_Message").removeClass('bg-warning');
                    $("#toast_Message").removeClass('bg-danger');
                    $("#toast_Message").removeClass('bg-info');

                    $("#toast_Message").addClass('bg-success');

                    $("#toast_Message").html("Item added successfully.");

                }


                else if (code == "upload") {


                    $("#toast_Message").removeClass('bg-success');
                    $("#toast_Message").removeClass('bg-warning');
                    $("#toast_Message").removeClass('bg-danger');
                    $("#toast_Message").removeClass('bg-info');

                    $("#toast_Message").addClass('bg-success');

                    $("#toast_Message").html("Item(s) uploaded successfully.");

                }

                else if (code == "update") {


                    $("#toast_Message").removeClass('bg-success');
                    $("#toast_Message").removeClass('bg-warning');
                    $("#toast_Message").removeClass('bg-danger');
                    $("#toast_Message").removeClass('bg-info');

                    $("#toast_Message").addClass('bg-warning');

                    $("#toast_Message").html("Item updated successfully.");

                }


                else if (code == "load") {


                    $("#toast_Message").removeClass('bg-success');
                    $("#toast_Message").removeClass('bg-warning');
                    $("#toast_Message").removeClass('bg-danger');
                    $("#toast_Message").removeClass('bg-info');

